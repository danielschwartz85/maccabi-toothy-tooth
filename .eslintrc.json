{
  "env": {
    "browser": true,
    "es2021": true
  },
  "overrides": [
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "plugins": [
      "@typescript-eslint",
      "prettier"
  ],
  "extends": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"]
  }
}
